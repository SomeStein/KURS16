**free
ctl-opt nomain;
dcl-pr ScanString ;
    inputString varchar(100) const;
    searchString varchar(20) const;
    result int(10);
end-pr;

dcl-proc ScanString export;
    dcl-pi *n;
        inputString varchar(100) const;
        searchString varchar(20) const;
        result int(10);
    end-pi;
    result = %scan(searchString : inputString);
end-proc;